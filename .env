# Database (Update with your local PostgreSQL connection)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/biye_db

# Redis (Update with your local Redis connection)
REDIS_URL=redis://localhost:6379

# JWT Secret (minimum 32 characters - CHANGE THIS IN PRODUCTION)
JWT_SECRET=development-secret-key-minimum-32-characters-long

# Server
NODE_ENV=development
PORT=5000

# Logging
LOG_LEVEL=info

# CORS - comma-separated list of allowed origins
ALLOWED_ORIGINS=http://localhost:5000,http://localhost:3000
